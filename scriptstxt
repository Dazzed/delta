CREATE TABLE EVENT_USER(
	EVENT_USER LONG NOT NULL,
	first_name VARCHAR2(100),
	last_name VARCHAR2(100),
	email VARCHAR2(100),
	phone VARCHAR2(100)
)

CREATE TABLE EVENT(
	EVENT_ID LONG NOT NULL,
	NAME VARCHAR2(100),
	SPONSOR VARCHAR2(100),
	EVENT_DATE VARCHAR2(100),
	ROOMS VARCHAR2(100),
	GUESTS VARCHAR2(100),
	IS_FLEXIBLE VARCHAR2(100),
	IS_REPEATING VARCHAR2(100)
)


CREATE TABLE EVENT_LOCATION(
	LOCATION_ID LONG NOT NULL,
	EVENT_LOCATION VARCHAR2(100),
	PREFERRED_HOTEL VARCHAR2(100),
	EVENT_BUDGET VARCHAR2(100),
	LAST_YEAR_HOTEL VARCHAR2(100)
)

CREATE TABLE EVENT_NEEDS(
	NEEDS_ID LONG NOT NULL,
	CATERING VARCHAR2(100),
	PARKING VARCHAR2(100),
	CONFERENCE_ROOMS VARCHAR2(100),
	AV_EQUIPMENT VARCHAR2(100),
	EARLY_CHECKIN VARCHAR2(100),
	LATE_CHECKOUT VARCHAR2(100),
	TRANSPORTATION VARCHAR2(100),
	TYPE VARCHAR2(100),
	COMMENTS VARCHAR2(100)
)

CREATE TABLE EVENT_DETAILS(
	EVENT_DETAILS_ID LONG NOT NULL,
	EVENT_ID VARCHAR2(100) NOT NULL,
	USER_ID VARCHAR2(100) NOT NULL,
	LOCATION_ID VARCHAR2(100) NOT NULL,
	NEEDS_ID VARCHAR2(100) NOT NULL
)




select * from EVENT

